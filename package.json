{
  "name": "Mary-Ann-Wildcat-Portfolio",
  "description": "An Astra child theme for Mary Ann Wildcat's Portfolio",
  "version": "1.0.0",
  "private": true,
  "license": "GPL-2.0-or-later",
  "repository": {
    "type": "git",
    "url": "git@github.com:mikeallen3/Mary-Ann-Wildcat-Portfolio.git"
  },
  "scripts": {
    "build-css": "node-sass  --watch assets -o dist --source-map-embed",
    "build-css-prod": "node-sass --output-style compressed assets -o dist",
    "autoprefixer": "postcss --no-map -u autoprefixer  -o dist/main.css dist/main.css",
    "watch": "nodemon  --watch assets -e scss, -x 'npm run build-css && npm run autoprefixer'",
    "build-prod": "npm run build-css-prod && npm run autoprefixer"
  },
  "dependencies": {
    "browserslist": "^4.6.6",
    "autoprefixer": "^9.6.1",
    "nodemon": "^1.19.1",
    "node-sass": "^4.12.0",
    "postcss-cli": "^6.1.3"
  },
  "browserslist": [
    "defaults"
  ]
}
